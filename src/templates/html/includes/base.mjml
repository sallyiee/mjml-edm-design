          <mj-column>
                <mj-image width="109" align="left" src="$IMGBASE$/logo.png" />
          </mj-column>
          <mj-column>
                <mj-image width="109" align="right" src="$IMGBASE$/logo.png" />
          </mj-column>
        </mj-group>
      </mj-section> -->

      <!-- Single Logo -->
      <mj-section
      background-url="$IMGBASE$/lendlease-header.jpg"
      background-size="cover"
      padding-bottom="0"
      >
          <mj-column class="header-height" >
                <mj-image width="143" align="left" src="$IMGBASE$/logo.png" />
                <mj-text>
                  <div class="header-divider"></div>
                  <h1 class="header-heading">National Property Showcase</h1>
                </mj-text>
          </mj-column>
      </mj-section>

